<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="PwdGestureView">
        <attr name="pgv_column_count" format="integer" />
        <attr name="pgv_row_count" format="integer" />

        <attr name="pgv_normal_oval_stroke_width" format="dimension" />
        <attr name="pgv_normal_oval_stroke_color" format="color|integer" />
        <attr name="pgv_normal_oval_stroke_select_color" format="color|integer" />
        <attr name="pgv_normal_oval_stroke_error_color" format="color|integer" />
        <attr name="pgv_normal_oval_solid_color" format="color|integer" />
        <attr name="pgv_normal_oval_solid_select_color" format="color|integer" />
        <attr name="pgv_normal_oval_solid_error_color" format="color|integer" />
        <attr name="pgv_normal_oval_radius" format="dimension" />

        <attr name="pgv_select_oval_stroke_width" format="dimension" />
        <attr name="pgv_select_oval_stroke_color" format="color|integer" />
        <attr name="pgv_select_oval_stroke_select_color" format="color|integer" />
        <attr name="pgv_select_oval_stroke_error_color" format="color|integer" />
        <attr name="pgv_select_oval_solid_color" format="color|integer" />
        <attr name="pgv_select_oval_solid_select_color" format="color|integer" />
        <attr name="pgv_select_oval_solid_error_color" format="color|integer" />
        <attr name="pgv_select_oval_radius" format="dimension" />

        <attr name="pgv_show_gesture_line" format="boolean" />
        <attr name="pgv_line_color" format="color|integer" />
        <attr name="pgv_line_width" format="dimension" />

        <attr name="pgv_auto_match" format="boolean" />
        <attr name="pgv_auto_reset_delay" format="integer" />

        <attr name="pgv_match_failed_color" format="color|integer" />
        <attr name="pgv_min_select_count" format="integer" />
    </declare-styleable>

</resources>

